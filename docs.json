{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Morpheus API Gateway",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "essentials/settings",
              "essentials/navigation"
            ]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "api-reference/introduction",
              "api-reference/quickstart"
            ]
          },
          {
            "group": "Authentication",
            "pages": [
              "api-reference/auth/me",
              "api-reference/auth/list-keys",
              "api-reference/auth/create-key",
              "api-reference/auth/delete-key"
            ]
          },
          {
            "group": "Models",
            "pages": [
              "api-reference/models/list",
              "api-reference/models/rated-bids"
            ]
          },
          {
            "group": "Chat",
            "pages": [
              "api-reference/chat/completions"
            ]
          },
          {
            "group": "Session Management",
            "pages": [
              "api-reference/session/approve",
              "api-reference/session/create-bid-session",
              "api-reference/session/create-model-session",
              "api-reference/session/close"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://morpheus.mintlify.com/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://mor.org/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:devs@mor.org"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "mcp",
     "cursor",
     "chatgpt",
     "claude",
     "perplexity",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/morpheusais",
      "github": "https://github.com/morpheusais",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}
