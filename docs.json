{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Morpheus API Gateway",
  "openapi": "/api-reference/openapi.json",
  "integrations": {
    "ga4": {
      "measurementId": "G-D0TCTKQ051"
    }
  },
  "api": {
    "baseUrl": "https://api.mor.org",
    "auth": {
      "method": "bearer"
    },
    "playground": {
      "proxy": false
    },
    "mdx": {
      "server": "https://api.mor.org"
    }
  },
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "documentation/getting-started/what-is-api-gateway",
              "documentation/getting-started/how-to-use-api-gateway"
            ]
          },
          {
            "group": "How to guides",
            "pages": [
              "documentation/how-to/creating-api-key",
              "documentation/how-to/viewing-models"
            ]
          },
          {
            "group": "Integration guides",
            "pages": [
              "documentation/integrations/opencode",
              "documentation/integrations/vercel-ai-sdk-integration",
              "documentation/integrations/openai-python-sdk",
              "documentation/integrations/cursor",
              "documentation/integrations/brave-leo",
              "documentation/integrations/open-web-ui",
              "documentation/integrations/eliza"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "api-reference/introduction",
              "api-reference/quickstart"
            ]
          },
          {
            "group": "Models",
            "pages": [
              "api-reference/models/list",
              "api-reference/models/all-models",
              "api-reference/models/rated-bids"
            ]
          },
          {
            "group": "Chat",
            "pages": [
              "api-reference/chat/completions"
            ]
          },
          {
            "group": "Embeddings",
            "pages": [
              "api-reference/embeddings/create-embeddings"
            ]
          },
          {
            "group": "Audio",
            "pages": [
              "api-reference/audio/speech"
            ]
          },
          {
            "group": "Utility",
            "pages": [
              "api-reference/utility/root",
              "api-reference/utility/health",
              "api-reference/utility/health-models"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://mor.org/blog",
          "icon": "newspaper"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/kyVaxTHnvB",
          "icon": "discord"
        },
        {
          "anchor": "Announcements",
          "href": "https://x.com/morpheusais",
          "icon": "x"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/Morpheus-white-bg.png",
    "dark": "/logo/Morpheus-black-bg.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:devs@mor.org"
      }
    ]
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "mcp",
     "cursor",
     "chatgpt",
     "claude",
     "perplexity",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/morpheusais",
      "github": "https://github.com/morpheusais",
      "linkedin": "https://linkedin.com/company/mintlify",
      "discord": "https://discord.gg/kyVaxTHnvB"
    }
  }
}
